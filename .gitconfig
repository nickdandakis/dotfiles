[core]
  pager = delta
[interactive]
  diffFilter = delta --color-only
[delta]
  navigate = true  # use n and N to move between diff sections
  line-numbers = true
[merge]
  conflictstyle = diff3
[commit]
  gpgsign = true
[tag]
  gpgsign = true
[diff]
  colorMoved = default
[pull]
  rebase = true
[user]
  email = nickdandakis@gmail.com
  name = Nick Dandakis
[push]
	autoSetupRemote = true
